<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buzzvil.buzzad.demo">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/icon_buzzvil"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_buzzvil"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sampleflow">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.buzzvil.APP_KEY"
            android:value="app-pub-100000043" />
    </application>

</manifest>